{
  "vim_mode": false,
  "disable_ai": false,
  "use_system_path_prompts": false,
  "base_keymap": "Cursor",
  "confirm_quit": true,
  "minimap": {
    "show": "auto"
  },
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Ayu Dark"
  },
  "languages": {
    "CSS": {
      "enable_language_server": false
    }
  },
  "experimental.theme_overrides": {
    "editor.background": "#16161b",

    "editor.gutter.background": "#16161b",

    "editor.active_line.background": "#16161b",

    "panel.background": "#16161b",

    "toolbar.background": "#16161b",

    "tab_bar.background": "#16161b",

    "title_bar.background": "#16161b",

    "status_bar.background": "#16161b",

    "tab.active_background": "#16161b",

    "tab.inactive_background": "#16161b",

    "terminal.background": "#16161b",

    "scrollbar.track.background": "#16161b",

    "scrollbar.thumb.background": "#16161b00",

    "border": "#ffffff1a",

    "border.variant": "#ffffff1a",

    "border.focused": "#ffffff1a",

    "border.disabled": "#ffffff1a",

    "elevated_surface.background": "#16161b",

    "surface.background": "#16161b",

    "background": "#16161b"
  },
  "gutter": {
    "line_numbers": true
  },

  "tab_bar": {
    "show": true
  },

  "project_panel": {
    "dock": "left",

    "indent_guides": {
      "show": "never"
    }
  }
}
