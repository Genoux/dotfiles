{
  "theme": "myTheme",
  "search": {
    "delay": 50,
    "placeholder": "Search applications..."
  },
  "ui": {
    "width": 600,
    "height": 400,
    "margin": 0,
    "anchors": {
      "top": false,
      "left": false,
      "right": false,
      "bottom": false
    },
    "show_initial_entries": true,
    "show_search": true,
    "fullscreen": true
  },
  "modules": [
    {
      "name": "applications",
      "prefix": "",
      "src_options": {
        "desktop_actions": false,
        "max_entries": 0
      }
    },
    {
      "name": "calculator",
      "prefix": "="
    },
    {
      "name": "finder",
      "prefix": "/"
    },
    {
      "name": "runner",
      "prefix": "!"
    },
    {
      "name": "websearch",
      "prefix": "?"
    },
    {
      "name": "clipboard",
      "prefix": "clip"
    },
    {
      "name": "emojis",
      "prefix": ":"
    }
  ],
  "builtins": {
    "ai": {
      "enabled": true,
      "anthropic": {
        "api_key_cmd": "echo $ANTHROPIC_API_KEY",
        "prompts": [
          {
            "label": "General",
            "prompt": "You are a helpful assistant. Keep answers concise."
          },
          {
            "label": "Code Help",
            "prompt": "You are a coding assistant. Provide clear, practical solutions."
          }
        ]
      }
    }
  },
  "external": [],
  "list": {
    "height": 200,
    "always_show": true,
    "max_entries": 20
  },
  "websearch": {
    "prefix": "?",
    "engines": {
      "google": "https://www.google.com/search?q=%s",
      "duckduckgo": "https://duckduckgo.com/?q=%s"
    }
  },
  "applications": {
    "hide_console": true
  },
  "clipboard": {
    "max_entries": 10,
    "prefix": "clip"
  }
}