{
  "theme": "main",
  "search": {
    "delay": 50,
    "placeholder": "Search applications..."
  },
  "ui": {
    "width": 600,
    "height": 400,
    "margin": 0,
    "anchors": {
      "top": false,
      "left": false,
      "right": false,
      "bottom": false
    },
    "show_initial_entries": false,
    "show_search": true,
    "fullscreen": true
  },
  "modules": [
    {
      "name": "applications",
      "prefix": "",
      "src_options": {
        "desktop_actions": false,
        "max_entries": 0
      }
    },
    {
      "name": "runner",
      "prefix": ">"
    },
    {
      "name": "calc",
      "prefix": "="
    }
  ],
  "external": [],
  "list": {
    "height": 200,
    "always_show": true,
    "max_entries": 20
  },
  "websearch": {
    "prefix": "?",
    "engines": {
      "google": "https://www.google.com/search?q=%s",
      "duckduckgo": "https://duckduckgo.com/?q=%s"
    }
  },
  "clipboard": {
    "max_entries": 10,
    "prefix": "clip"
  }
}