# btop theme (Generated by flavours from {{scheme-name}})

# Main background, empty for terminal default/transparent
theme[main_bg]="#{{base01-hex}}"

# Main text color
theme[main_fg]="#e9e9e9"

# Title color for boxes
theme[title]="#f0fdf4"

# Highlight color for keyboard shortcuts (gray for consistency with terminal apps)
theme[hi_fg]="#{{base05-hex}}"

# Background color of selected item in processes box (gray for consistency)
theme[selected_bg]="#{{base04-hex}}"

# Foreground color of selected item in processes box
theme[selected_fg]="#{{base05-hex}}"

# Color of inactive/disabled text
theme[inactive_fg]="#{{base03-hex}}"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#{{base04-hex}}"

# Cpu box outline color
theme[cpu_box]="#35332d"

# Memory/disks box outline color
theme[mem_box]="#35332d"

# Net up/down box outline color
theme[net_box]="#35332d"

# Processes box outline color
theme[proc_box]="#35332d"

# Box divider line and small boxes line color
theme[div_line]="#35332d"

# Temperature graph colors
theme[temp_start]="#15803c"
theme[temp_mid]="#{{base0B-hex}}"
theme[temp_end]="#dcfce7"

# CPU graph colors
theme[cpu_start]="#86efab"
theme[cpu_mid]="#bbf7d0"
theme[cpu_end]="#f0fdf4"

# Mem/Disk free meter
theme[free_start]="#15803c"
theme[free_mid]="#{{base0B-hex}}"
theme[free_end]="#f0fdf4"

# Mem/Disk cached meter
theme[cached_start]="#15803c"
theme[cached_mid]="#{{base0B-hex}}"
theme[cached_end]="#dcfce7"

# Mem/Disk available meter
theme[available_start]="#15803c"
theme[available_mid]="#{{base0B-hex}}"
theme[available_end]="#dcfce7"

# Mem/Disk used meter
theme[used_start]="#15803c"
theme[used_mid]="#{{base0B-hex}}"
theme[used_end]="#dcfce7"

# Download graph colors
theme[download_start]="#15803c"
theme[download_mid]="#{{base0B-hex}}"
theme[download_end]="#dcfce7"

# Upload graph colors
theme[upload_start]="#15803c"
theme[upload_mid]="#{{base0B-hex}}"
theme[upload_end]="#dcfce7"
