# ZSH Syntax Highlighting Colors
# Generated from {{scheme-name}} by {{scheme-author}}
# Source this file in your .zshrc: source ~/.config/zsh/highlight-colors.zsh

# Override syntax highlighting colors with theme
ZSH_HIGHLIGHT_STYLES[default]='none'
ZSH_HIGHLIGHT_STYLES[unknown-token]='fg=#{{base08-hex}},bold'
ZSH_HIGHLIGHT_STYLES[reserved-word]='fg=#{{base0B-hex}},bold'
ZSH_HIGHLIGHT_STYLES[alias]='fg=#{{base0B-hex}},bold'
ZSH_HIGHLIGHT_STYLES[suffix-alias]='fg=#{{base0B-hex}},bold'
ZSH_HIGHLIGHT_STYLES[global-alias]='fg=#{{base0B-hex}},bold'
ZSH_HIGHLIGHT_STYLES[builtin]='fg=#{{base0B-hex}},bold'
ZSH_HIGHLIGHT_STYLES[function]='fg=#{{base0D-hex}},bold'
ZSH_HIGHLIGHT_STYLES[command]='fg=#{{base0B-hex}},bold'
ZSH_HIGHLIGHT_STYLES[precommand]='fg=#{{base0E-hex}},italic'
ZSH_HIGHLIGHT_STYLES[commandseparator]='fg=#{{base04-hex}}'
ZSH_HIGHLIGHT_STYLES[hashed-command]='fg=#{{base0B-hex}}'
ZSH_HIGHLIGHT_STYLES[path]='fg=#{{base0F-hex}},underline'
ZSH_HIGHLIGHT_STYLES[path_prefix]='fg=#{{base0E-hex}},underline'
ZSH_HIGHLIGHT_STYLES[path_approx]='fg=#{{base0E-hex}},underline'
ZSH_HIGHLIGHT_STYLES[globbing]='fg=#{{base0D-hex}},bold'
ZSH_HIGHLIGHT_STYLES[history-expansion]='fg=#{{base0D-hex}},bold'
ZSH_HIGHLIGHT_STYLES[single-quoted-argument]='fg=#{{base0B-hex}}'
ZSH_HIGHLIGHT_STYLES[double-quoted-argument]='fg=#{{base0B-hex}}'
ZSH_HIGHLIGHT_STYLES[dollar-quoted-argument]='fg=#{{base0B-hex}}'
ZSH_HIGHLIGHT_STYLES[dollar-double-quoted-argument]='fg=#{{base0D-hex}}'
ZSH_HIGHLIGHT_STYLES[back-quoted-argument]='fg=#{{base0D-hex}}'
ZSH_HIGHLIGHT_STYLES[redirection]='fg=#{{base0D-hex}},bold'
ZSH_HIGHLIGHT_STYLES[arg0]='fg=#{{base0B-hex}}'
ZSH_HIGHLIGHT_STYLES[comment]='fg=#{{base03-hex}},italic'
ZSH_HIGHLIGHT_STYLES[assign]='fg=#{{base04-hex}}'

