# ESP32 boards - CP210x (Silicon Labs), ESP32 native, CH340

SUBSYSTEMS=="usb", ATTRS{idVendor}=="10c4", MODE="0666", GROUP="uucp"

SUBSYSTEMS=="usb", ATTRS{idVendor}=="303a", MODE="0666", GROUP="uucp"

SUBSYSTEMS=="usb", ATTRS{idVendor}=="1a86", MODE="0666", GROUP="uucp"

# Auto-load cp210x module when device is detected

ACTION=="add", SUBSYSTEM=="usb", ATTRS{idVendor}=="10c4", RUN+="/sbin/modprobe cp210x"

